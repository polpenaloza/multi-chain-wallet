  "scripts": {
    "build": "next build",
    "check:lint": "next lint --fix",
    "check:types": "tsc --pretty --noEmit",
    "clean": "pnpm clean:dist && pnpm clean:eslint && pnpm clean:next && pnpm clean:out && pnpm clean:node",
    "clean:dist": "find . -name dist | xargs rm -rf",
    "clean:eslint": "find . -name .eslintcache | xargs rm -rf",
    "clean:next": "find . -name .next | xargs rm -rf",
    "clean:node": "find . -name node_modules | xargs rm -rf",
    "clean:out": "find . -name out | xargs rm -rf",
    "clean:quick": "find . -name node_modules -o -name .next -o -name dist -type d -prune | xargs rm -rf",
    "dev": "next dev --turbopack",
    "format": "prettier . --write",
    "maintenance": "pnpm clean:quick && pnpm install",
    "nuke": "pnpm clean && rm pnpm-lock.yaml && pnpm install",
    "prepare": "husky",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "version": "0.1.0"
